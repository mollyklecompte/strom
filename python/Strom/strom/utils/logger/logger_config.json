{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(filename)s - %(funcName)s - %(lineno)d - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "file":{
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename": "strom.log"
    }
  },
  "loggers": {
    "stromLogger": {
      "level": "DEBUG",
      "handlers": ["console", "file"],
      "propagate": "no"
    },
    "root": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}
